<odoo>
  <data>
    <record model="ir.ui.view" id="adb-general.pos_list">
      <field name="name">POS Terminals</field>
      <field name="model">adb.pos</field>
      <field name="arch" type="xml">
        <tree>
          <field name="bank_id"/>
          <field name="location_id"/>
          <field name="currency"/>
          <field name="phone"/>
          <field name="date_deployed"/>
          <field name="merchant_id"/>
          <field name="email"/>
        </tree>
      </field>
    </record>

    <record model="ir.actions.act_window" id="adb-general.action_pos_list">
      <field name="name">POS Terminals</field>
      <field name="res_model">adb.pos</field>
      <field name="view_mode">tree</field>
    </record>

  </data>
</odoo>