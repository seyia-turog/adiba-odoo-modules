<odoo>
  <data>

    <record model="ir.ui.view" id="adb-general.adb_banks_local">
      <field name="name">Local Banks</field>
      <field name="model">res.bank</field>
      <field name="arch" type="xml">
        <tree>
          <field name="location_id"/>
          <!-- <field name="nip_code"/> -->
          <field name="name"/>
          <!-- <field name="cbn_code"/> -->
          <!-- <field name="bank_logo"/> -->
          <field name="country"/>
          <field name="category"/>
          <field name="description"/>
          <field name="swift_code"/>
          <field name="branch_count"/>
          <field name="incorporation_date"/>
          <field name="rc_number"/>
          <field name="website"/>
        </tree>
      </field>
    </record>

    <record model="ir.actions.act_window" id="adb-general.adb_banks_local_list">
      <field name="name">Banks</field>
      <field name="res_model">res.bank</field>
      <field name="view_mode">tree</field>
    </record>


  </data>
</odoo>