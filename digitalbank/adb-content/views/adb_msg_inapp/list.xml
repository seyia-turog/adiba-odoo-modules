<odoo>
  <data>
    <record model="ir.ui.view" id="adb-content.msg_inapp">
      <field name="name">In App Messages</field>
      <field name="model">adb.msg.inapp</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="client_id"/>
          <field name="broadcast_rank"/>
          <field name="type"/>
        </tree>
      </field>
    </record>

    <record model="ir.actions.act_window" id="adb-content.action_messages_list">
      <field name="name">In App Messages</field>
      <field name="res_model">adb.msg.inapp</field>
      <field name="view_mode">tree</field>
    </record>

  </data>
</odoo>