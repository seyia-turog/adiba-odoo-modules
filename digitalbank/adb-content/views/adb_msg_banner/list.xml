<odoo>
  <data>
    <record model="ir.ui.view" id="adb-content.msg_banner">
      <field name="name">Banners &amp; Ads</field>
      <field name="model">adb.msg.banner</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="type"/>
          <field name="group"/>
        </tree>
      </field>
    </record>

    <record model="ir.actions.act_window" id="adb-content.action_banners_list">
      <field name="name">Banners &amp; Ads</field>
      <field name="res_model">adb.msg.banner</field>
      <field name="view_mode">tree</field>
    </record>

  </data>
</odoo>